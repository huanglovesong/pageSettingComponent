{
  "presets": [
    "env",
    "stage-0",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    [
      "import", [
        {
          "libraryName": "antd-mobile",
          "style": true
        }
      ]
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        "dva-hmr"
      ]
    },
    "production": {
      "plugins": [
        [
          "transform-react-remove-prop-types", {
            "ignoreFilenames": ["node_modules"]
          }
        ]
      ]
    }
  }
}
